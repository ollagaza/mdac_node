      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties",
      "require-context-hook",
      [
        "module-resolver",
        {
          "alias": {
